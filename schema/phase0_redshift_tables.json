{ "tables": {
  "previous_sessions": {
    "description" : "Session information from previous executions.",
    "timestamp_column": "start_time",
    "days": 2,
    "source_table": "sessions",
    "exclude": ["end_time"],
    "expire_after_phase": 3
    },
  "previous_requests": {
    "description" : "Request information from previous executions.",
    "timestamp_column": "timestamp",
    "days": 2,
    "source_table": "requests",
    "exclude": ["timestamp_year", "timestamp_month", "timestamp_day"],
    "expire_after_phase": 1
    }
  }
}